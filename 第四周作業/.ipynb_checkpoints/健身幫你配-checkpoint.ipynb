    "import random\n",
    "workout = {\"仰臥起坐\":7.17, \"伏地挺身\":6, \"跳繩\":8.4, \"慢跑\":4.7}\n",
    "\n",
    "burn = int(input(\"今天想消耗多少熱量?(kcal) \"))\n",
    "w = random.choice(list(workout.keys()))\n",
    "result = round(burn / workout[w])\n",
    "print(\"哇!感覺今天可以\" + w + str(result) + \"分鐘唷\")"
   ]
